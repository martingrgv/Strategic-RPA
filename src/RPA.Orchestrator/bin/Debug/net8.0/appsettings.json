{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Orchestrator": {
    "DefaultAgentCount": 3,
    "JobProcessingIntervalSeconds": 5,
    "HealthCheckIntervalMinutes": 2,
    "MaintenanceIntervalHours": 4
  },
  "RDP": {
    "BasePort": 3390
  },
  "HealthMonitor": {
    "AgentHeartbeatTimeoutMinutes": 5,
    "SessionInactivityTimeoutHours": 2,
    "MaxJobsPerSession": 50,
    "JobTimeoutMinutes": 30,
    "MaxCompletedJobsToKeep": 1000
  },
  "JobScheduler": {
    "MaxRetryCount": 3,
    "DefaultJobTimeoutMinutes": 30,
    "RetryDelaySeconds": 60,
    "HighPriorityThreshold": 8,
    "MediumPriorityThreshold": 5
  }
}
